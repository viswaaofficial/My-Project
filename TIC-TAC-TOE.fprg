    <function name="MainMenu" type="None" variable="">
        <parameters/>
        <body>
            <declare name="option" type="Integer" array="False" size=""/>
            <declare name="return" type="String" array="False" size=""/>
            <output expression="&quot;******* Main Menu *******&quot;" newline="True"/>
            <output expression="&quot;***** 1: One vs One *****&quot;" newline="True"/>
            <output expression="&quot;*** 2: One vs Computer ***&quot;" newline="True"/>

    <function name="COUNTC" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
            <parameter name="pos" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="i, COUNT, j, row, column, FULL" type="Integer" array="False" size=""/>
            <declare name="MOVE, RESULT, oppos" type="String" array="False" size=""/>
            <assign variable="i" expression="0"/>
            <assign variable="MOVE" expression="&quot;TEMP&quot;"/>
            <assign variable="RESULT" expression="&quot;TEMP&quot;"/>
            <while expression="i&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;OVER&quot;">
                <assign variable="FULL" expression="0"/>
                <assign variable="COUNT" expression="0"/>
                <assign variable="row" expression="i"/>
                <for variable="j" start="0" end="n-1" direction="inc" step="1">
                    <if expression="T[n*i+j]==pos">
                        <then>
                            <assign variable="COUNT" expression="COUNT+1"/>
                        </then>
                        <else/>
                    </if>
                </for>
                <if expression="COUNT==2">
                    <then>
                        <for variable="j" start="0" end="n-1" direction="inc" step="1">
                            <if expression="T[n*i+j]==&quot;_&quot;">
                                <then>
                                    <assign variable="column" expression="j"/>
                                </then>
                                <else>
                                    <assign variable="FULL" expression="FULL+1"/>
                                </else>
                            </if>
                        </for>
                        <if expression="FULL!=3">
                            <then>
                                <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                            </then>
                            <else/>
                        </if>
                    </then>
                    <else/>
                </if>
                <assign variable="i" expression="i+1"/>
            </while>
            <if expression="MOVE!=&quot;OVER&quot;">
                <then>
                    <assign variable="j" expression="0"/>
                    <while expression="j&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;OVER&quot;">
                        <assign variable="FULL" expression="0"/>
                        <assign variable="COUNT" expression="0"/>
                        <assign variable="column" expression="j"/>
                        <for variable="i" start="0" end="n-1" direction="inc" step="1">
                            <if expression="T[n*i+j]==pos">
                                <then>
                                    <assign variable="COUNT" expression="COUNT+1"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="COUNT==2">
                            <then>
                                <for variable="i" start="0" end="n-1" direction="inc" step="1">
                                    <if expression="T[n*i+j]==&quot;_&quot;">
                                        <then>
                                            <assign variable="row" expression="i"/>
                                        </then>
                                        <else>
                                            <assign variable="FULL" expression="FULL+1"/>
                                        </else>
                                    </if>
                                </for>
                                <if expression="FULL!=3">
                                    <then>
                                        <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                        <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                                    </then>
                                    <else/>
                                </if>
                            </then>
                            <else/>
                        </if>
                        <assign variable="j" expression="j+1"/>
                    </while>
                </then>
                <else/>
            </if>
            <if expression="RESULT!=&quot;OVER&quot;&amp;&amp;MOVE!=&quot;OVER&quot;">
                <then>
                    <if expression="pos==&quot;X&quot;">
                        <then>
                            <assign variable="oppos" expression="&quot;O&quot;"/>
                        </then>
                        <else>
                            <assign variable="oppos" expression="&quot;X&quot;"/>
                        </else>
                    </if>
                    <assign variable="RESULT" expression="COUNTO(T,n,pos,oppos)"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="COUNTCORN" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
            <parameter name="pos" type="String" array="False"/>
            <parameter name="oppos" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="i, j, COUNTEMP, row, column" type="Integer" array="False" size=""/>
            <declare name="RESULT, MOVE" type="String" array="False" size=""/>
            <assign variable="COUNTEMP" expression="0"/>
            <for variable="i" start="0" end="2" direction="inc" step="2">
                <for variable="j" start="0" end="2" direction="inc" step="2">
                    <if expression="T[n*i+j]==&quot;_&quot;">
                        <then>
                            <assign variable="COUNTEMP" expression="COUNTEMP+1"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <assign variable="MOVE" expression="&quot;NOT OVER&quot;"/>
            <assign variable="RESULT" expression="&quot;NOT OVER&quot;"/>
            <if expression="pos==&quot;O&quot;">
                <then>
                    <if expression="COUNTEMP==4">
                        <then>
                            <assign variable="row" expression="0"/>
                            <assign variable="column" expression="0"/>
                            <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                            <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                        </then>
                        <else>
                            <if expression="COUNTEMP==3">
                                <then>
                                    <assign variable="row" expression="1"/>
                                    <assign variable="column" expression="1"/>
                                    <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                    <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                                </then>
                                <else/>
                            </if>
                        </else>
                    </if>
                    <if expression="COUNTEMP&lt;3&amp;&amp;RESULT!=&quot;OVER&quot;&amp;&amp;MOVE!=&quot;OVER&quot;">
                        <then>
                            <assign variable="i" expression="0"/>
                            <while expression="RESULT!=&quot;OVER&quot;&amp;&amp;i&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;">
                                <assign variable="j" expression="0"/>
                                <while expression="j&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;">
                                    <if expression="T[n*i+j]==&quot;_&quot;">
                                        <then>
                                            <assign variable="row" expression="i"/>
                                            <assign variable="column" expression="j"/>
                                            <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                            <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                                        </then>
                                        <else/>
                                    </if>
                                    <assign variable="j" expression="j+2"/>
                                </while>
                                <assign variable="i" expression="i+2"/>
                            </while>
                        </then>
                        <else/>
                    </if>
                </then>
                <else>
                    <if expression="COUNTEMP&lt;=4&amp;&amp;RESULT!=&quot;OVER&quot;&amp;&amp;MOVE!=&quot;OVER&quot;">
                        <then>
                            <assign variable="i" expression="0"/>
                            <while expression="RESULT!=&quot;0VER&quot;&amp;&amp;i&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;">
                                <assign variable="j" expression="0"/>
                                <while expression="j&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;">
                                    <if expression="T[n*i+j]==&quot;_&quot;">
                                        <then>
                                            <assign variable="row" expression="i"/>
                                            <assign variable="column" expression="j"/>
                                            <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                            <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                                        </then>
                                        <else/>
                                    </if>
                                    <assign variable="j" expression="j+2"/>
                                </while>
                                <assign variable="i" expression="i+2"/>
                            </while>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="COUNTD" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
            <parameter name="pos" type="String" array="False"/>
            <parameter name="oppos" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="i, j, COUNTD1, COUNTD2, row, column" type="Integer" array="False" size=""/>
            <declare name="RESULT, MOVE, mark" type="String" array="False" size=""/>
            <assign variable="RESULT" expression="&quot;TEMP&quot;"/>
            <assign variable="MOVE" expression="&quot;NOT OVER&quot;"/>
            <assign variable="mark" expression="pos"/>
            <assign variable="j" expression="0"/>
            <do expression="mark==oppos&amp;&amp;j&lt;2">
                <assign variable="COUNTD1" expression="0"/>
                <assign variable="COUNTD2" expression="0"/>
                <for variable="i" start="0" end="n-1" direction="inc" step="1">
                    <if expression="T[n*i+i]==mark">
                        <then>
                            <assign variable="COUNTD1" expression="COUNTD1+1"/>
                            <if expression="T[n*(i+1)-(i+1)]==mark">
                                <then>
                                    <assign variable="COUNTD2" expression="COUNTD2+1"/>
                                </then>
                                <else/>
                            </if>
                        </then>
                        <else/>
                    </if>
                </for>
                <if expression="COUNTD1==2">
                    <then>
                        <for variable="i" start="0" end="n-1" direction="inc" step="1">
                            <if expression="T[n*i+i]==&quot;_&quot;">
                                <then>
                                    <assign variable="row" expression="i"/>
                                    <assign variable="column" expression="i"/>
                                    <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                    <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                    </then>
                    <else>
                        <if expression="COUNTD2==2">
                            <then>
                                <for variable="i" start="0" end="n-1" direction="inc" step="1">
                                    <if expression="T[n*(i+1)-i]==&quot;_&quot;">
                                        <then>
                                            <assign variable="row" expression="i"/>
                                            <assign variable="column" expression="i"/>
                                            <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                            <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                                        </then>
                                        <else/>
                                    </if>
                                </for>
                            </then>
                            <else>
                                <assign variable="RESULT" expression="&quot;NOT OVER&quot;"/>
                                <assign variable="MOVE" expression="&quot;NOT OVER&quot;"/>
                            </else>
                        </if>
                    </else>
                </if>
                <if expression="MOVE!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;OVER&quot;">
                    <then>
                        <assign variable="mark" expression="oppos"/>
                    </then>
                    <else/>
                </if>
                <assign variable="j" expression="j+1"/>
            </do>
            <if expression="MOVE!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;OVER&quot;">
                <then>
                    <assign variable="RESULT" expression="COUNTCORN(T,n,pos,oppos)"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="COUNTO" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
            <parameter name="pos" type="String" array="False"/>
            <parameter name="oppos" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="i, COUNT, j, row, column, FULL" type="Integer" array="False" size=""/>
            <declare name="MOVE, RESULT" type="String" array="False" size=""/>
            <assign variable="i" expression="0"/>
            <assign variable="MOVE" expression="&quot;TEMP&quot;"/>
            <assign variable="RESULT" expression="&quot;TEMP&quot;"/>
            <while expression="i&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;OVER&quot;">
                <assign variable="FULL" expression="0"/>
                <assign variable="COUNT" expression="0"/>
                <assign variable="row" expression="i"/>
                <for variable="j" start="0" end="n-1" direction="inc" step="1">
                    <if expression="T[n*i+j]==oppos">
                        <then>
                            <assign variable="COUNT" expression="COUNT+1"/>
                        </then>
                        <else/>
                    </if>
                </for>
                <if expression="COUNT==2">
                    <then>
                        <for variable="j" start="0" end="n-1" direction="inc" step="1">
                            <if expression="T[n*i+j]==&quot;_&quot;">
                                <then>
                                    <assign variable="column" expression="j"/>
                                </then>
                                <else>
                                    <assign variable="FULL" expression="FULL+1"/>
                                </else>
                            </if>
                        </for>
                        <if expression="FULL!=3">
                            <then>
                                <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                            </then>
                            <else/>
                        </if>
                    </then>
                    <else/>
                </if>
                <assign variable="i" expression="i+1"/>
            </while>
            <if expression="MOVE!=&quot;OVER&quot;">
                <then>
                    <assign variable="j" expression="0"/>
                    <while expression="j&lt;n&amp;&amp;MOVE!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;OVER&quot;">
                        <assign variable="FULL" expression="0"/>
                        <assign variable="COUNT" expression="0"/>
                        <assign variable="column" expression="j"/>
                        <for variable="i" start="0" end="n-1" direction="inc" step="1">
                            <if expression="T[n*i+j]==oppos">
                                <then>
                                    <assign variable="COUNT" expression="COUNT+1"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="COUNT==2">
                            <then>
                                <for variable="i" start="0" end="n-1" direction="inc" step="1">
                                    <if expression="T[n*i+j]==&quot;_&quot;">
                                        <then>
                                            <assign variable="row" expression="i"/>
                                        </then>
                                        <else>
                                            <assign variable="FULL" expression="FULL+1"/>
                                        </else>
                                    </if>
                                </for>
                                <if expression="FULL!=3">
                                    <then>
                                        <assign variable="RESULT" expression="ASSIGN(T,n,row,column,pos)"/>
                                        <assign variable="MOVE" expression="&quot;OVER&quot;"/>
                                    </then>
                                    <else/>
                                </if>
                            </then>
                            <else/>
                        </if>
                        <assign variable="j" expression="j+1"/>
                    </while>
                </then>
                <else/>
            </if>
            <if expression="RESULT!=&quot;OVER&quot;&amp;&amp;MOVE!=&quot;OVER&quot;">
                <then>
                    <assign variable="RESULT" expression="COUNTD(T,n,pos,oppos)"/>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="CPLR" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
            <parameter name="pos" type="String" array="False"/>

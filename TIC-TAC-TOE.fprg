    <function name="Match" type="None" variable="">
        <parameters>
            <parameter name="Pos1" type="String" array="False"/>
            <parameter name="Pos2" type="String" array="False"/>
            <parameter name="Name1" type="String" array="False"/>
            <parameter name="Name2" type="String" array="False"/>
        </parameters>
        <body>
            <output expression="&quot;Enter Number of Rounds&quot;" newline="True"/>
            <declare name="rounds, R, Scr1, Scr2" type="Integer" array="False" size=""/>
            <input variable="rounds"/>
            <output expression="&quot;*** It's Important to read the instructions ***&quot;" newline="True"/>
            <call expression="Rules"/>
            <output expression="&quot;********** Let's Begin **********&quot;" newline="True"/>
            <declare name="Winner" type="String" array="False" size=""/>
            <assign variable="Scr1" expression="0"/>
            <assign variable="Scr2" expression="0"/>
            <for variable="R" start="1" end="rounds" direction="inc" step="1">
                <output expression="&quot;ROUND &quot;&amp;R" newline="True"/>
                <declare name="n, i, j" type="Integer" array="False" size=""/>
                <if expression="Name1==&quot;Computer&quot;||Name2==&quot;Computer&quot;">
                    <then>
                        <assign variable="n" expression="3"/>
                    </then>
                    <else>
                        <output expression="&quot;PLEASE MENTION YOUR PREFERRED GRID LENGTH&quot;" newline="True"/>
                        <input variable="n"/>
                    </else>
                </if>
                <declare name="T" type="String" array="True" size="n*n"/>
                <for variable="i" start="0" end="(n*n)-1" direction="inc" step="1">
                    <assign variable="T[i]" expression="&quot;_&quot;"/>
                </for>
                <call expression="DISPLAY(T,n)"/>
                <if expression="Name2!=&quot;Computer&quot;&amp;&amp;Name1!=&quot;Computer&quot;">
                    <then>
                        <assign variable="Winner" expression="RESULT(Name1,Name2,T,n)"/>
                    </then>
                    <else>
                        <assign variable="Winner" expression="RESULTC(Name1,Name2,T,n)"/>
                    </else>
                </if>
                <if expression="Winner==Name1">
                    <then>
                        <assign variable="Scr1" expression="Scr1+1"/>
                    </then>
                    <else>
                        <if expression="Winner==Name2">
                            <then>
                                <assign variable="Scr2" expression="Scr2+1"/>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
                <output expression="&quot;****************************************&quot;" newline="True"/>
                <output expression="Name1&amp;&quot; : &quot;&amp;Scr1" newline="True"/>
                <output expression="Name2&amp;&quot; : &quot;&amp;Scr2" newline="True"/>
                <output expression="&quot;****************************************&quot;" newline="True"/>
            </for>
            <if expression="Scr1&gt;Scr2">
                <then>
                    <output expression="&quot;****************************************&quot;" newline="True"/>
                    <output expression="&quot;*** &quot;&amp;Name1&amp;&quot; is the WINNER of the TOURNAMENT ***&quot;" newline="True"/>
                    <output expression="&quot;****************************************&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="Scr2&gt;Scr1">
                        <then>
                            <output expression="&quot;****************************************&quot;" newline="True"/>
                            <output expression="&quot;*** &quot;&amp;Name2&amp;&quot; is the WINNER of the TOURNAMENT ***&quot;" newline="True"/>
                            <output expression="&quot;****************************************&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;****************************************&quot;" newline="True"/>
                            <output expression="&quot;*** &quot;&amp;Name1&amp;&quot; and &quot;&amp;Name2&amp;&quot; is the WINNER of the TOURNAMENT ***&quot;" newline="True"/>
                            <output expression="&quot;****************************************&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>

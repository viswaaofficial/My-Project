            </do>
        </body>
    </function>
    <function name="RESULT" type="String" variable="Name">
        <parameters>
            <parameter name="Name1" type="String" array="False"/>
            <parameter name="Name2" type="String" array="False"/>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="RESULT, Name" type="String" array="False" size=""/>
            <declare name="Scr" type="Integer" array="True" size="2"/>
            <assign variable="Scr[0]" expression="0"/>
            <assign variable="Scr[1]" expression="0"/>
            <do expression="RESULT!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;DRAW&quot;">
                <assign variable="RESULT" expression="PLR1(T,n)"/>
                <assign variable="Name" expression="Name1"/>
                <call expression="DISPLAY(T,n)"/>
                <if expression="RESULT!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;DRAW&quot;">
                    <then>
                        <assign variable="RESULT" expression="PLR2(T,n)"/>
                        <assign variable="Name" expression="Name2"/>
                        <call expression="DISPLAY(T,n)"/>
                    </then>
                    <else/>
                </if>
                <if expression="RESULT==&quot;DRAW&quot;">
                    <then>
                        <output expression="&quot;GAME IS A DRAW&quot;" newline="True"/>
                        <assign variable="Name" expression="&quot;None&quot;"/>
                    </then>
                    <else/>
                </if>
                <if expression="RESULT==&quot;OVER&quot;">
                    <then>
                        <output expression="&quot;Congrats &quot;&amp;Name&amp;&quot; You won the game&quot;" newline="True"/>
                    </then>
                    <else/>
                </if>
            </do>
        </body>
    </function>
    <function name="RESULTC" type="String" variable="NAME">
        <parameters>
            <parameter name="Name1" type="String" array="False"/>
            <parameter name="Name2" type="String" array="False"/>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="RESULT, Name, pos" type="String" array="False" size=""/>
            <declare name="Scr" type="Integer" array="True" size="2"/>
            <assign variable="Scr[0]" expression="0"/>
            <assign variable="Scr[1]" expression="0"/>
            <do expression="RESULT!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;DRAW&quot;">
                <if expression="Name1==&quot;Computer&quot;">
                    <then>
                        <assign variable="pos" expression="&quot;X&quot;"/>
                        <assign variable="RESULT" expression="CPLR(T,n,pos)"/>
                        <assign variable="Name" expression="Name1"/>
                        <call expression="DISPLAY(T,n)"/>
                        <if expression="RESULT!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;DRAW&quot;">
                            <then>
                                <assign variable="RESULT" expression="PLR2(T,n)"/>
                                <assign variable="Name" expression="Name2"/>
                                <call expression="DISPLAY(T,n)"/>
                            </then>
                            <else/>
                        </if>
                        <if expression="RESULT==&quot;DRAW&quot;">
                            <then>
                                <output expression="&quot;GAME IS A DRAW&quot;" newline="True"/>
                                <assign variable="Name" expression="&quot;None&quot;"/>
                            </then>
                            <else/>
                        </if>
                        <if expression="RESULT==&quot;OVER&quot;">
                            <then>
                                <output expression="&quot;Congrats &quot;&amp;Name&amp;&quot; You won the game&quot;" newline="True"/>
                            </then>
                            <else/>
                        </if>
                    </then>
                    <else>
                        <assign variable="pos" expression="&quot;O&quot;"/>
                        <assign variable="RESULT" expression="PLR1(T,n)"/>
                        <assign variable="Name" expression="Name1"/>
                        <call expression="DISPLAY(T,n)"/>
                        <if expression="RESULT!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;DRAW&quot;">
                            <then>
                                <assign variable="RESULT" expression="CPLR(T,n,pos)"/>
                                <assign variable="Name" expression="Name2"/>
                                <call expression="DISPLAY(T,n)"/>
                            </then>
                            <else/>
                        </if>
                        <if expression="RESULT==&quot;DRAW&quot;">
                            <then>
                                <output expression="&quot;GAME IS A DRAW&quot;" newline="True"/>
                                <assign variable="Name" expression="&quot;None&quot;"/>
                            </then>
                            <else/>
                        </if>
                        <if expression="RESULT==&quot;OVER&quot;">
                            <then>
                                <output expression="&quot;Congrats &quot;&amp;Name&amp;&quot; You won the game&quot;" newline="True"/>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
            </do>
        </body>
    </function>
    <function name="Rules" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;1. The game is played on a grid that's n squares by n squares&quot;" newline="True"/>
            <output expression="&quot;2. You are X, your friend (or the computer in this case) is O. Players take turns putting their marks in empty squares.&quot;" newline="True"/>
            <output expression="&quot;3. Player assigned as X Starts first&quot;" newline="True"/>
            <output expression="&quot;4. The first player to get n marks in a row (up, down, across, or diagonally) is the winner.&quot;" newline="True"/>
            <output expression="&quot;5. If it's a draw NO POINTS is provided&quot;" newline="True"/>
            <output expression="&quot;6. When all nxn squares are full, the game is over.&quot;" newline="True"/>
        </body>
    </function>
    <function name="TOSS" type="None" variable="">
        <parameters>
            <parameter name="Player1" type="String" array="False"/>
            <parameter name="Player2" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="Option, RandOption" type="Integer" array="False" size=""/>
            <declare name="Pos1, Pos2, temp" type="String" array="False" size=""/>
            <output expression="&quot;*** 0:HEAD ***&quot;" newline="True"/>
            <output expression="&quot;*** 1:TAIL ***&quot;" newline="True"/>
            <output expression="Player1&amp;&quot; CHOOSE ANY ONE OPTION&quot;" newline="True"/>
            <input variable="option"/>
            <assign variable="RandOption" expression="Random(2)"/>
            <if expression="Option==RandOption">
                <then>
                    <output expression="Player1&amp;&quot; Won the toss and You are Assigned as X and &quot;&amp;Player2&amp;&quot; is Assigned as O&quot;" newline="True"/>
                    <assign variable="Pos1" expression="&quot;X&quot;"/>
                    <assign variable="Pos2" expression="&quot;O&quot;"/>
                </then>
                <else>
                    <output expression="Player1&amp;&quot; Lost the toss and You are Assigned as O and &quot;&amp;Player2&amp;&quot; is Assigned as X&quot;" newline="True"/>
                    <assign variable="Pos1" expression="&quot;O&quot;"/>
                    <assign variable="Pos2" expression="&quot;X&quot;"/>
                    <assign variable="temp" expression="Player1"/>
                    <assign variable="Player1" expression="Player2"/>
                    <assign variable="Player2" expression="temp"/>
                </else>
            </if>
            <output expression="&quot;Player 1 (X) : &quot;&amp;Player1" newline="True"/>
            <output expression="&quot;Player 2 (O) : &quot;&amp;Player2" newline="True"/>
            <call expression="Match(Pos1,Pos2,Player1,Player2)"/>
        </body>
    </function>

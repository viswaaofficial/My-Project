    <function name="ASSIGN" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
            <parameter name="row" type="Integer" array="False"/>
            <parameter name="column" type="Integer" array="False"/>
            <parameter name="pos" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="RESULT, QUERY" type="String" array="False" size=""/>
            <do expression="QUERY==&quot;INVALID&quot;">
                <assign variable="QUERY" expression="&quot;TEMP&quot;"/>
                <if expression="T[n*row+column]==&quot;_&quot;">
                    <then>
                        <assign variable="T[n*row+column]" expression="pos"/>
                        <assign variable="RESULT" expression="CHECK(T,n)"/>
                    </then>
                    <else>
                        <output expression="&quot;INVALID INPUT PLEASE ENTER AGAIN&quot;" newline="False"/>
                        <assign variable="QUERY" expression="&quot;INVALID&quot;"/>
                    </else>
                </if>
            </do>
        </body>
    </function>
    <function name="CHECK" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="RESULT, TEMPRES" type="String" array="False" size=""/>
            <assign variable="RESULT" expression="CHECKROW(T,n)"/>
            <assign variable="TEMPRES" expression="&quot;TEMP&quot;"/>
            <if expression="RESULT!=&quot;OVER&quot;">
                <then>
                    <if expression="RESULT==&quot;DRAW&quot;">
                        <then>
                            <assign variable="TEMPRES" expression="&quot;DRAW&quot;"/>
                        </then>
                        <else/>
                    </if>
                    <assign variable="RESULT" expression="CHECKCLM(T,n)"/>
                    <if expression="RESULT==&quot;NOTOVER&quot;&amp;&amp;TEMPRES==&quot;DRAW&quot;">
                        <then>
                            <assign variable="RESULT" expression="TEMPRES"/>
                        </then>
                        <else/>
                    </if>
                    <if expression="RESULT!=&quot;OVER&quot;">
                        <then>
                            <if expression="RESULT==&quot;DRAW&quot;">
                                <then>
                                    <assign variable="TEMPRES" expression="&quot;DRAW&quot;"/>
                                </then>
                                <else/>
                            </if>
                            <assign variable="RESULT" expression="CHECKDIG(T,n)"/>
                            <if expression="RESULT==&quot;NOTOVER&quot;&amp;&amp;TEMPRES==&quot;DRAW&quot;">
                                <then>
                                    <assign variable="RESULT" expression="TEMPRES"/>
                                </then>
                                <else/>
                            </if>
                        </then>
                        <else/>
                    </if>
                </then>
                <else/>
            </if>
        </body>
    </function>
    <function name="CHECKCLM" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="i, j, sum" type="Integer" array="False" size=""/>
            <declare name="RESULT" type="String" array="False" size=""/>
            <assign variable="j" expression="0"/>
            <assign variable="RESULT" expression="&quot;TEMP&quot;"/>
            <while expression="RESULT!=&quot;OVER&quot;&amp;&amp;RESULT!=&quot;DRAW&quot;&amp;&amp;j&lt;n">
                <assign variable="RESULT" expression="&quot;TEMP&quot;"/>
                <assign variable="sum" expression="0"/>
                <assign variable="i" expression="0"/>
                <while expression="RESULT!=&quot;NOTOVER&quot;&amp;&amp;i&lt;n-1">
                    <if expression="T[n*i+j]==T[n*(i+1)+j]">
                        <then>
                            <if expression="T[n*i+j]!=&quot;_&quot;&amp;&amp;T[n*(i+1)+j]!=&quot;_&quot;">
                                <then>
                                    <assign variable="RESULT" expression="&quot;OVER&quot;"/>
                                </then>
                                <else/>
                            </if>
                        </then>
                        <else>
                            <assign variable="RESULT" expression="&quot;NOTOVER&quot;"/>
                        </else>
                    </if>
                    <assign variable="i" expression="i+1"/>
                </while>
                <if expression="RESULT==&quot;OVER&quot;">
                    <then/>
                    <else>
                        <for variable="i" start="0" end="n-1" direction="inc" step="1">
                            <for variable="j" start="0" end="n-1" direction="inc" step="1">
                                <if expression="T[n*i+j]!=&quot;_&quot;">
                                    <then>
                                        <assign variable="sum" expression="sum+1"/>
                                    </then>
                                    <else/>
                                </if>
                            </for>
                        </for>
                        <if expression="sum==n*n">
                            <then>
                                <assign variable="RESULT" expression="&quot;DRAW&quot;"/>
                            </then>
                            <else>
                                <assign variable="RESULT" expression="&quot;NOTOVER&quot;"/>
                            </else>
                        </if>
                    </else>
                </if>
                <assign variable="j" expression="j+1"/>
            </while>
        </body>
    </function>
    <function name="CHECKDIG" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="i, j, sum" type="Integer" array="False" size=""/>
            <declare name="RESULT, TEMPRES" type="String" array="False" size=""/>
            <assign variable="i" expression="0"/>
            <assign variable="RESULT" expression="&quot;TEMP&quot;"/>
            <assign variable="TEMPRES" expression="&quot;TEMPVAL&quot;"/>
            <assign variable="sum" expression="0"/>
            <while expression="RESULT!=&quot;NOTOVER&quot;&amp;&amp;i&lt;n-1">
                <if expression="T[n*i+i]==T[n*(i+1)+i+1]">
                    <then>
                        <if expression="T[n*i+i]!=&quot;_&quot;&amp;&amp;T[n*(i+1)+i+1]!=&quot;_&quot;">
                            <then>
                                <assign variable="RESULT" expression="&quot;OVER&quot;"/>
                            </then>
                            <else/>
                        </if>
                    </then>
                    <else>
                        <assign variable="RESULT" expression="&quot;NOTOVER&quot;"/>
                    </else>
                </if>
                <assign variable="i" expression="i+1"/>
            </while>
            <if expression="RESULT==&quot;OVER&quot;">
                <then/>
                <else>
                    <for variable="i" start="0" end="n-1" direction="inc" step="1">
                        <for variable="j" start="0" end="n-1" direction="inc" step="1">
                            <if expression="T[n*i+j]!=&quot;_&quot;">
                                <then>
                                    <assign variable="sum" expression="sum+1"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                    </for>
                    <if expression="sum==(n*n)">
                        <then>
                            <assign variable="RESULT" expression="&quot;DRAW&quot;"/>
                        </then>
                        <else>
                            <assign variable="RESULT" expression="&quot;NOTOVER&quot;"/>
                        </else>
                    </if>
                </else>
            </if>
            <if expression="RESULT!=&quot;OVER&quot;">
                <then>
                    <if expression="RESULT==&quot;DRAW&quot;">
                        <then>
                            <assign variable="TEMPRES" expression="&quot;DRAW&quot;"/>
                        </then>
                        <else/>
                    </if>
                    <assign variable="RESULT" expression="CHECKDIG2(T,n)"/>
                    <if expression="RESULT==&quot;NOTOVER&quot;&amp;&amp;TEMPRES==&quot;DRAW&quot;">
                        <then>
                            <assign variable="RESULT" expression="TEMPRES"/>
                        </then>
                        <else/>
                    </if>
                </then>
                <else/>
            </if>
        </body>
    </function>

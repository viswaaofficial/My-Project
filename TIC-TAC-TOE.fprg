    <function name="PLR1" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <output expression="&quot;PLAYER 1 TURN&quot;" newline="True"/>
            <declare name="row, column" type="Integer" array="False" size=""/>
            <declare name="RESULT, QUERY" type="String" array="False" size=""/>
            <do expression="QUERY==&quot;INVALID&quot;">
                <input variable="row"/>
                <input variable="column"/>
                <assign variable="QUERY" expression="&quot;TEMP&quot;"/>
                <if expression="T[n*row+column]==&quot;_&quot;">
                    <then>
                        <assign variable="T[n*row+column]" expression="&quot;X&quot;"/>
                        <assign variable="RESULT" expression="CHECK(T,n)"/>
                    </then>
                    <else>
                        <output expression="&quot;INVALID INPUT PLEASE ENTER AGAIN&quot;" newline="True"/>
                        <assign variable="QUERY" expression="&quot;INVALID&quot;"/>
                    </else>
                </if>
            </do>
        </body>
    </function>
    <function name="PLR2" type="String" variable="RESULT">
        <parameters>
            <parameter name="T" type="String" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <output expression="&quot;PLAYER 2 TURN&quot;" newline="True"/>
            <declare name="row, column" type="Integer" array="False" size=""/>
            <declare name="RESULT, QUERY" type="String" array="False" size=""/>
            <do expression="QUERY==&quot;INVALID&quot;">
                <input variable="row"/>
                <input variable="column"/>
                <assign variable="QUERY" expression="&quot;TEMP&quot;"/>
                <if expression="T[n*row+column]==&quot;_&quot;">
                    <then>
                        <assign variable="T[n*row+column]" expression="&quot;O&quot;"/>
                        <assign variable="RESULT" expression="CHECK(T,n)"/>
                    </then>
                    <else>
                        <output expression="&quot;INVALID INPUT PLEASE ENTER AGAIN&quot;" newline="False"/>
                        <assign variable="QUERY" expression="&quot;INVALID&quot;"/>
                    </else>
